<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CurrentlyPlayingActivity"
            android:label="@string/currently_playing"
            android:parentActivityName=".SongsActivity"></activity>
        <activity
            android:name=".SongsActivity"
            android:label="@string/organize_songs"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".ArtistsActivity"
            android:label="@string/organize_artists"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".AlbumsActivity"
            android:label="@string/organize_albums"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>